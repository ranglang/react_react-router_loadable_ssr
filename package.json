    "build": "better-npm-run build",
    "devel": "better-npm-run devel",
    "start": "better-npm-run start"
  },
  "betterScripts": {
    "build": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app*",
        "PORT": "3000"
      }
    },
    "devel": {
      "command": "node --inspect --require @babel/register ./src/server.js",
